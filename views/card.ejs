<meta http-equiv="refresh" content="5">
<title>RFID management</title>
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-wEmeIV1mKuiNpC+IOBjI7aAzPcEZeedi5yW5f2yOq55WWLwNGmvvx4Um1vskeMj0" crossorigin="anonymous">
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-p34f1UUtsS3wqzfto5wAAmdvj+osOnFyQFpp4Ua3gs/ZVWx6oOypYoCJhGGScy+8" crossorigin="anonymous"></script>
<body >
  <table class="table" class="">
      <thead class="thead-dark">
        <tr style="background-color: rgb(70, 233, 132);color: white;">
          <th scope="col">Card Id</th>
          <th scope="col">Initial balance</th>
          <th scope="col">Transport Fare</th>
          <th scope="col">New balance</th>
          <th scope="col">card Owner</th>
        </tr>
      </thead>
      <tbody>
        <% cardDetails.forEach(function(card) { %>
        <tr>
          <td><%= card.card_id %></td>
          <td><%= card.amount %></td>
          <td><%= card.fare %></td>
          <td><%= card.final_Amount %></td>
          <td><%= card.card_owner %></td>
        </tr>
        <% }); %>
      </tbody>
    </table>
</body>